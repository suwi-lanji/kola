{
    "preset": "laravel",
    "rules": {
        "simplified_null_return": true,
        "braces": false,
        "new_with_braces": true,
        "no_unused_imports": true,
        "no_extra_blank_lines": true,
        "ordered_imports": true,
        "single_import_per_statement": true,
        "strict_comparison": true,
        "concat_space": {
            "spacing": "one"
        }
    },
    "exclude": [
        "bootstrap/cache",
        "storage",
        "vendor"
    ]
}